{
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "procesar_csv.settings"
  },
  "hooks": {
    "build": "python manage.py collectstatic --noinput"
  }
}
